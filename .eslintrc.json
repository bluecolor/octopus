{
  "globals": {
    "define": true,
    "Ladda": true,
    "BootstrapDialog": true, 
    "waitingDialog": true,
    "Chart": true,
    "moment": true,
    "zxcvbn": true,
    "daterangepicker":true 
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn"
  }
}